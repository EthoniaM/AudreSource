@page "{id:int}"
@model AudreSource.Pages.DeleteKanBanDetailModel



<div class="card">
    <div class="card-body">
        <h3>Are you sure you want to delete this KanBan detail?</h3>
        <div>
            <dl class="row">
                <dt class="col-sm-3">Comments</dt>
                <dd class="col-sm-9">@Model.KanBanDetail??Comments</dd>
                <dt class="col-sm-3">Task Title</dt>
                <dd class="col-sm-9">@Model.KanBanDetail??Kanban.TaskTitle</dd>
                <dt class="col-sm-3">User</dt>
                <dd class="col-sm-9">@Model.KanBanDetail??User.FirstNames @Model.KanBanDetail??User.Surname</dd>
            </dl>
        </div>
        <form method="post">
            <input type="hidden" asp-for="KanBanDetail.KanBanDetailsID" />
            <button type="submit" class="btn btn-danger">Delete</button>
            <a class="btn btn-secondary" asp-page="/KanBanDetailList">Back to List</a>
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
